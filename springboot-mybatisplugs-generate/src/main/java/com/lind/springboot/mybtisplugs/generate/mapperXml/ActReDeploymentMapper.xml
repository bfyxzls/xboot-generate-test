<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.lind.springboot.mybtisplugs.generate.mapper.ActReDeploymentMapper">

	<!-- 通用查询映射结果 -->
	<resultMap id="BaseResultMap" type="com.lind.springboot.mybtisplugs.generate.entity.ActReDeployment">
		<id column="ID_" property="id" />
		<result column="NAME_" property="name" />
		<result column="CATEGORY_" property="category" />
		<result column="TENANT_ID_" property="tenantId" />
		<result column="DEPLOY_TIME_" property="deployTime" />
	</resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        ID_ AS id, NAME_ AS name, CATEGORY_ AS category, TENANT_ID_ AS tenantId, DEPLOY_TIME_ AS deployTime
    </sql>

</mapper>

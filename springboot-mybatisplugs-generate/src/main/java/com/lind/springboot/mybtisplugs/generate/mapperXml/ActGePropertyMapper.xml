<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.lind.springboot.mybtisplugs.generate.mapper.ActGePropertyMapper">

	<!-- 通用查询映射结果 -->
	<resultMap id="BaseResultMap" type="com.lind.springboot.mybtisplugs.generate.entity.ActGeProperty">
		<id column="NAME_" property="name" />
		<result column="VALUE_" property="value" />
		<result column="REV_" property="rev" />
	</resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        NAME_ AS name, VALUE_ AS value, REV_ AS rev
    </sql>

</mapper>
